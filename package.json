{
    "name": "asymptote",
    "displayName": "Asymptote",
    "description": "Support for Asymptote, a vector graphics language.",
    "version": "1.2.0",
    "publisher": "supakornras",
    "repository": {
        "type": "git",
        "url": "https://github.com/supakorn-ras/vscode-lang-asymptote"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:asymptote"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "asymptote",
                "aliases": [
                    "Asymptote",
                    "asymptote"
                ],
                "extensions": [
                    ".asy"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "breakpoints": [
            {
                "language": "asymptote"
            }
        ],
        "grammars": [
            {
                "language": "asymptote",
                "scopeName": "source.asymptote",
                "path": "./syntaxes/asymptote.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "@types/node": "^15.0.2",
        "@types/vscode": "^1.56.0",
        "vscode-languageclient": "^7.0.0"
    },
    "scripts": {
        "build": "tsc -p ./"
    }
}
